!!!

%html{:lang => "en"}

  %head
    %meta{:charset => "utf-8"}
    %meta{:content => "IE=edge", "http-equiv" => "X-UA-Compatible"}
    %meta{:content => "width=device-width, initial-scale=1", :name => "viewport"}

    %title NetSearch Direct Report Demo

    %link{:href => "http://fonts.googleapis.com/css?family=Roboto:300,400,500,700,400italic", :rel => "stylesheet"}
    -#%link{:href => "/assets/css/dashboard-light.css", :rel => "stylesheet" }
    -#%link{:href => "/assets/css/dashboard-application.css", :rel => "stylesheet" }
    -#%link{:href => "/assets/css/custom-dashboard.css", :rel => "stylesheet" }

  %body.with-top-navbar
    %nav.navbar.navbar-inverse.navbar-fixed-top
      .container-fluid.container-fluid-spacious
        .navbar-header
          %button.navbar-toggle.collapsed{"aria-controls" => "navbar", "aria-expanded" => "false", "data-target" => "#navbar", "data-toggle" => "collapse", :type => "button"}
            %span.sr-only Toggle navigation
            %span.icon-bar
            %span.icon-bar
            %span.icon-bar
          %a.navbar-brand.navbar-brand-emphasized{:href => ""} NetSearch Direct
        #navbar.navbar-collapse.collapse
          %ul.nav.navbar-nav
            %li.active
              %a{ :href => "" } PPC Dashboard
            %li
              %a{ :href => "" } SEO Dashboard
    .container-fluid.container-fluid-spacious
      .dashhead.m-t-md
        .dashhead-titles
          %h2.dashhead-title Acme Corp. PPC Dashboard
        .btn-toolbar.dashhead-toolbar
          .btn-toolbar-item.input-with-icon
            %input.form-control{"data-provide" => "datepicker", :type => "text", :value => "01/01/15 - 01/08/15"}
              %span.icon.icon-calendar
      .hr-divider.m-t-lg.m-b-md
        %h3.hr-divider-content.hr-divider-heading Financial Metrics
      .row.statcards.financial-metrics
        .col-md-6.m-b-md
          .statcard
            .p-a.leads
              %span.statcard-desc Total Leads
              %h2.statcard-number
                51
                %small.label.label-success 5%
            .p-a.forms
              %span.statcard-desc Forms
              %h2.statcard-number
                %span.label &nbsp;
                30
            .p-a.calls
              %span.statcard-desc Calls
              %h2.statcard-number
                %span.label &nbsp;
                21
            %canvas.dashboard-chart.ex-bar-graph.leads-chart{:width => "378",
              :height => "94",
              :style => "width: 189px; height: 47px;"}
        .col-md-6.m-b-md
          .statcard
            .p-a.cost
              %span.statcard-desc Cost
              %h2.statcard-number
                $2,539.32
                %small.label.label-danger 1.3%
            .p-a.cost-per-lead
              %span.statcard-desc Cost per Lead
              %h2.statcard-number $49.79
            %canvas.dashboard-chart.ex-line-graph.cost-chart{:width => "378",
              :height => "94",
              :style => "width: 189px; height: 47px;"}
      .hr-divider.m-t-lg.m-b-md
        %h3.hr-divider-content.hr-divider-heading Funnel Metrics
      .row.statcards.funnel-metrics
        .col-md-3.m-b-md
          .statcard
            .p-a
              %span.statcard-desc Impressions
              %h2.statcard-number 8,511
            %canvas.dashboard-chart.ex-line-graph.impressions-chart{:width => "378",
              :height => "94",
              :style => "width: 189px; height: 47px;"}
        .col-md-3.m-b-md
          .statcard
            .p-a
              %span.statcard-desc Click Through Rate
              %h2.statcard-number 5.38%

%nav.navbar.navbar-default.navbar-fixed-top
  .container-fluid.container-fluid-spacious
    .navbar-header
      %button.navbar-toggle.collapsed{"aria-controls" => "navbar", "aria-expanded" => "false", "data-target" => "#navbar", "data-toggle" => "collapse", :type => "button"}
        %span.sr-only Toggle navigation
        %span.icon-bar
        %span.icon-bar
        %span.icon-bar
      =link_to image_tag("netsearch-logo-small.png"), "http://www.netsearchdirect.com", :class => "navbar-brand"
    #navbar.navbar-collapse.collapse
      %ul.nav.navbar-nav
        %li.active
          %a{ :href => "" } PPC Dashboard
        %li
          %a{ :href => "" } SEO Dashboard
      %ul.nav.navbar-nav.logout
        %li
          =button_to 'Log Out', logout_path, method: :delete, class: "btn btn-default-outline"
.container-fluid.container-fluid-spacious
  .dashhead.m-t-md
    .dashhead-titles
      %h2.dashhead-title
        =@client.name
        \- PPC Dashboard
    .btn-toolbar.dashhead-toolbar
      .btn-toolbar-item
        .input-with-icon
          %input.date-picker.form-control{:type => "text"}
          %span.icon.icon-calendar
  .hr-divider.m-t-lg.m-b-md
    %h3.hr-divider-content.hr-divider-heading Financial Metrics
  .row.statcards.financial-metrics
    .col-md-6.m-b-md
      .statcard.leads-widget
        .p-a.total-leads
          %span.statcard-desc Total Leads
          %h2.statcard-number
            %span.chart-summary-metric 0
            -#%small.label.label-success 5%
        .p-a.forms
          %span.statcard-desc Forms
          %h2.statcard-number
            %span.label &nbsp;
            %span.chart-summary-metric 0
        .p-a.calls
          %span.statcard-desc Calls
          %h2.statcard-number
            %span.label &nbsp;
            %span.chart-summary-metric 0
        %canvas.dashboard-chart.ex-bar-graph.leads-chart{:width => "378",
          :height => "94",
          :style => "width: 189px; height: 47px;"}
    .col-md-6.m-b-md
      .statcard.cost-widget
        .p-a.cost
          %span.statcard-desc Cost
          %h2.statcard-number
            %span.chart-summary-metric $0.00
            -#%small.label.label-danger 1.3%
        .p-a.cost-per-lead
          %span.statcard-desc Cost per Lead
          %h2.statcard-number.chart-summary-metric $0.00
        %canvas.dashboard-chart.ex-line-graph.cost-chart{:width => "378",
          :height => "94",
          :style => "width: 189px; height: 47px;"}
